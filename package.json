{
  "name": "json-audit-tm",
  "version": "0.1.0",
  "description": "A lightweight TypeScript library to compare two JSON states and generate structured audit logs. It detects added, removed, and modified fields, with support for custom display names, array identity keys, and metadata such as actor and timestamp.",
  "keywords": [
    "json",
    "audit",
    "diff",
    "typescript",
    "npm",
    "react",
    "angular"
  ],
  "license": "MIT",
  "author": "Thiago (TM)",
  "repository": {
    "type": "git",
    "url": "https://github.com/<seu-usuario>/json-audit-tm.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "prepublishOnly": "npm run build && npm test",
    "sample": "node scripts/sample.mjs",
    "audit:hash": "node scripts/audit-hash.mjs",
    "audit": "npm run build && node scripts/audit-hash.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.42.0",
    "vitest": "^2.0.0"
  }
}